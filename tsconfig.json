{
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "forceConsistentCasingInFileNames": true,
    "baseUrl": "./src",
    "allowImportingTsExtensions": true,
    "noEmit": true,
    "paths": {
      "@firebase/*": ["./node_modules/@firebase/*"]
    },
    "jsx": "react"
  },
  "include": ["src"]
}
